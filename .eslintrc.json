{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["no-loops", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": 2,
    "no-loops/no-loops": 2
    // "prettier/prettier": 2
  }
}
